{
    "compilerOptions": {
        "module": "ESNext",
        "target": "ESNext",
        "moduleResolution": "Node",
        "baseUrl": ".",
        "paths": {
            "@/*": [
                "resources/*"
            ],
            "@react/*": [
                "resources/react/*"
            ]
        },
        "jsx": "react",
        "allowSyntheticDefaultImports": true
    }
}
